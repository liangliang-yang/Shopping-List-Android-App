<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seclass.gatech.edu.glm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:m_name=".MainActivity">
            <intent-filter>
                <action android:m_name="android.intent.action.MAIN" />

                <category android:m_name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:m_name=".ListActivity"></activity>
    </application>

</manifest>